                </structure>
            </list-property>
            <property name="queryText">SELECT  DECODE(ent.NUMERO_PROSA, 38,7, ent.NUMERO_PROSA) NUMERO_PROSA
       ,ent.NOMBRE
       ,SUM(DECODE(ps.TLI_NUMERO, 46, DECODE(ps.TTR_NUMERO,  1, (ps.TOTAL_IMPORTE - ps.TOTAL_COMISION)
                                                          , 21, (ps.TOTAL_IMPORTE - ABS(ps.TOTAL_COMISION)) * - 1
                                                          ,  2, (ps.TOTAL_IMPORTE + ps.TOTAL_COMISION)
                                                          , 25, (ps.TOTAL_IMPORTE - ps.TOTAL_COMISION) * - 1
                                                          , 26, (ps.TOTAL_IMPORTE - ps.TOTAL_COMISION)
                                                          , 27, (ps.TOTAL_IMPORTE + ps.TOTAL_COMISION) * - 1
                                                          ,  9, (ps.TOTAL_IMPORTE - ps.TOTAL_COMISION)
                                                          , 15, (ps.TOTAL_IMPORTE - ps.TOTAL_COMISION) * - 1
                                                          , 16, (ps.TOTAL_IMPORTE - ps.TOTAL_COMISION)
                                                          , 17, (ps.TOTAL_IMPORTE - ps.TOTAL_COMISION) * - 1
                                                          ,  3, (ps.TOTAL_IMPORTE - ps.TOTAL_COMISION)
                                                          , 22, (ps.TOTAL_IMPORTE - ps.TOTAL_COMISION) * - 1
                                                          , 10, (ps.TOTAL_IMPORTE - ps.TOTAL_COMISION)
                                                          , 11, (ps.TOTAL_IMPORTE - ps.TOTAL_COMISION) * -1
                                                          , 70, (ps.TOTAL_IMPORTE - ps.TOTAL_COMISION)
                                                          , 71, (ps.TOTAL_IMPORTE - ps.TOTAL_COMISION) * - 1
                                                          , 52, (ps.TOTAL_IMPORTE - ps.TOTAL_COMISION)
                                                          , 35, (ps.TOTAL_IMPORTE - ps.TOTAL_COMISION)
                                                          , 36, (ps.TOTAL_IMPORTE - ps.TOTAL_COMISION) * - 1
                                                          , 37, (ps.TOTAL_IMPORTE - ps.TOTAL_COMISION)
                                                          , 0
                                            ), 0
                  )
           ) IMP_VISA      ,
       SUM(DECODE(ps.TLI_NUMERO, 47, DECODE(ps.TTR_NUMERO,  1, (ps.TOTAL_IMPORTE - ps.TOTAL_COMISION)
                                                          , 21, (ps.TOTAL_IMPORTE - ABS(ps.TOTAL_COMISION)) * - 1
                                                          ,  2, (ps.TOTAL_IMPORTE + ps.TOTAL_COMISION)
                                                          , 25, (ps.TOTAL_IMPORTE - ps.TOTAL_COMISION) * - 1
                                                          , 26, (ps.TOTAL_IMPORTE - ps.TOTAL_COMISION)
                                                          , 27, (ps.TOTAL_IMPORTE + ps.TOTAL_COMISION) * - 1
                                                          ,  9, (ps.TOTAL_IMPORTE - ps.TOTAL_COMISION)
                                                          , 15, (ps.TOTAL_IMPORTE - ps.TOTAL_COMISION) * - 1
                                                          , 16, (ps.TOTAL_IMPORTE - ps.TOTAL_COMISION)
                                                          , 17, (ps.TOTAL_IMPORTE - ps.TOTAL_COMISION) * - 1
                                                          ,  3, (ps.TOTAL_IMPORTE - ps.TOTAL_COMISION)
                                                          , 22, (ps.TOTAL_IMPORTE - ps.TOTAL_COMISION) * - 1
                                                          , 10, (ps.TOTAL_IMPORTE - ps.TOTAL_COMISION)
                                                          , 11, (ps.TOTAL_IMPORTE - ps.TOTAL_COMISION) * -1
                                                          , 70, (ps.TOTAL_IMPORTE - ps.TOTAL_COMISION)
                                                          , 71, (ps.TOTAL_IMPORTE - ps.TOTAL_COMISION) * - 1
                                                          , 52, (ps.TOTAL_IMPORTE - ps.TOTAL_COMISION)
                                                          , 35, (ps.TOTAL_IMPORTE - ps.TOTAL_COMISION)
                                                          , 36, (ps.TOTAL_IMPORTE - ps.TOTAL_COMISION) * - 1
                                                          , 37, (ps.TOTAL_IMPORTE - ps.TOTAL_COMISION)
                                                          , 0
                                            ), 0
                  )
           ) IMP_MC
       --,0 IMP_ATM
--SELECT *
  FROM  supersic.POSICIONES@LG_PMTU_SIC ps
       ,supersic.ENTIDADES@LG_PMTU_SIC  ent
       --,ENTIDADES pa
 WHERE  ps.FECHA            BETWEEN TO_DATE(?, 'dd/mm/yyyy')
                                AND TO_DATE(?, 'dd/mm/yyyy')
   AND  ps.TLI_NUMERO            IN (46,47)
   AND  ent.NUMERO_PROSA IN(SELECT en1.NUMERO_PROSA
                              FROM supersic.ENTIDADES@LG_PMTU_SIC en1
                                  ,supersic.ENTIDADES@LG_PMTU_SIC en2
                             WHERE (    en2.NUMERO_PROSA = DECODE(?, 7, 38)
                                     OR en2.NUMERO_PROSA = DECODE(?, 7, 7, 0, en2.NUMERO_PROSA, ?)
                                   )
                               AND en1.ENT_NUMERO_PROSA = en2.ENT_NUMERO_PROSA
                               AND ((en2.IND_LIQ_VISA = '3' AND en2.IND_DCC IN('1')) OR (en2.IND_LIQ_VISA = '1' AND en2.IND_DCC IN('1')))
                               AND ((en1.IND_LIQ_VISA = '3' AND en1.IND_DCC IN('1')) OR (en1.IND_LIQ_VISA = '1' AND en1.IND_DCC IN('1')))
                          )
   AND  (ps.ENT_NUMERO_PROSA_ADQ  =  ent.NUMERO_PROSA OR ps.ENT_NUMERO_PROSA_EMI =  ent.NUMERO_PROSA)
--
 GROUP
    BY  ent.NUMERO_PROSA
       ,ent.NOMBRE
 ORDER
    BY  ent.NUMERO_PROSA</property>
